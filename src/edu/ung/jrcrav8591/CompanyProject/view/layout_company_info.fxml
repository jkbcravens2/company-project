<?import javafx.scene.layout.AnchorPane?>

<TabPane prefWidth="890.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ung.jrcrav8591.CompanyProject.controller.CompanyInfoController">
   <tabs>
      <Tab text="Employees">
         <content>
            <AnchorPane prefHeight="393.0" prefWidth="890.0">
               <children>
                  <Label layoutX="14.0" layoutY="27.0" text="Company Name: " />
                  <Label fx:id="company_name_label" layoutX="106.0" layoutY="27.0" text="Name" />
                  <Label layoutX="14.0" layoutY="44.0" text="Company Address:  " />
                  <Label fx:id="company_address_label" layoutX="120.0" layoutY="44.0" text="Address" />
                  <Label layoutX="14.0" layoutY="61.0" text="Company City: " />
                  <Label layoutX="163.0" layoutY="61.0" text="Company State: " />
                  <Label fx:id="company_city_label" layoutX="96.0" layoutY="61.0" text="City" />
                  <Label fx:id="company_state_label" layoutX="250.0" layoutY="61.0" text="State" />
                  <Label layoutX="349.0" layoutY="61.0" text="Company Zip: " />
                  <Label fx:id="company_zip_label" layoutX="426.0" layoutY="61.0" text="Zip" />

                  <Button fx:id="new_employee_buton" layoutX="795.0" layoutY="461.0" mnemonicParsing="false" onAction="#addNewEmployee" text="New Employee" />
                  <TableView layoutY="90.0" prefHeight="371.0" prefWidth="890.0">
                    <columns>
                      <TableColumn prefWidth="75.0" text="C1" />
                      <TableColumn prefWidth="75.0" text="C2" />
                    </columns>
                  </TableView>
               </children>
            </AnchorPane>
         </content>
      </Tab>
      <Tab text="Supervisors">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </Tab>
   </tabs>
</TabPane>
